@using ibrasOneriAnket.Models
@model List<KullaniciAnketCevap>


@{
    ViewBag.Title = "GenelSonuclar";
    Layout = "~/Views/Shared/_AdminControlLayout.cshtml";
}


<h2>Anket Sonuçlar</h2>

<br />

<!DOCTYPE html>

<html lang="en">

<head>

    <meta charset="UTF-8">

    <title>Pdf - Excel</title>
    <link href="~/Content/minFiles/bootstrap.css" rel="stylesheet" />
    <script src="~/Content/minFiles/dataTables.bootstrap4.min.js"></script>
    <link href="~/Content/minFiles/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/minFiles/responsive.bootstrap4.min.css" rel="stylesheet" />


</head>

<body>
    <table id="example" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">

        <thead>

            <tr>
                <th>Soru</th>
                <th>Cevap</th>
                <th>Cevaplanma Zamanı</th>
            </tr>

        </thead>

        <tbody>

            @foreach (var x in Model)

            {

                <tr>

                    <td>@x.SoruId</td>
                    <td>@x.CevapId</td>
                    <td>@x.CevaplamaZamani</td>
                </tr>
            }
        </tbody>

    </table>

    <script src="~/Content/minFiles/jquery-3.3.1.js"></script>
    <script src="~/Content/minFiles/jquery.dataTables.min.js"></script>
    <script src="~/Content/minFiles/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/minFiles/dataTables.buttons.min.js"></script>
    <link href="~/Content/minFiles/buttons.bootstrap4.min.css" rel="stylesheet" />
    <script src="~/Content/minFiles/jszip.min.js"></script>
    <script src="~/Content/minFiles/pdfmake.min.js"></script>
    <script src="~/Content/minFiles/vfs_fonts.js"></script>
    <script src="~/Content/minFiles/buttons.html5.min.js"></script>
    <script src="~/Content/minFiles/buttons.print.min.js"></script>
    <script src="~/Content/minFiles/buttons.colVis.min.js"></script>
    <script src="~/Content/minFiles/dataTables.responsive.min.js"></script>
    <link href="~/Content/minFiles/responsive.bootstrap4.min.css" rel="stylesheet" />

    <script>

        $(document).ready(function () {

            var table = $('#example').DataTable({

                lengthChange: false,

                buttons: ['copy', 'excel', 'csv', 'pdf', 'colvis']

            });



            table.buttons().container()

                .appendTo('#example_wrapper .col-md-6:eq(0)');

        });

    </script>

</body>

</html>















